$(document).ready(function(){if($("div.article").find("table").length>0){$("div.article").find("table").addClass("tablesaw tablesaw-stack").attr("data-mode","stack")
}var a=window.matchMedia("only screen and (max-width: 760px)");
if(a.matches){$("div.article table").each(function(){$(this).find("th").each(function(){var b=$(this).text();
var c=$(this).index();
$(this).parent().siblings("tr").each(function(){$(this).find("td").eq(c).prepend("<b class='tablesaw-cell-label'>"+b+"</b>")
})
})
})
}});